  "scripts": {
    "compile-production": "pnpm run prune-semantic-ui-to-used-selectors && webpack --mode production --config='webpack.dll.js' && webpack --config='webpack.prod.config.js'",
    "compile-test": "pnpm run prune-semantic-ui-to-used-selectors && webpack --config='webpack.dll.mjs' && NODE_ENV=test webpack --config='webpack.shared.config.js'",
    "watch": "node webpack.watch.js",
    "watch-semantic-ui": "nodemon -e jsx,heex --exec 'pnpm run prune-semantic-ui-to-used-selectors'",
    "prune-semantic-ui-to-used-selectors": "postcss --config ./postcss.config.cjs priv/static/css/semantic-ui/semantic.min.css --output priv/static/css/semantic-ui/semantic-ui-purged.min.css",
    "precommit": "lint-staged",
    "postinstall": "bin/prepare_semantic_ui_for_consumption_by_phoenix",
    "test": "mocha --config=./.mocharc.json --node-env=test --exit",
    "test:watch": "mocha --watch --watch-extensions jsx",
    "lint": "eslint --fix \"web/static/js/**/*.{js,jsx}\" \"test/**/*.{js,jsx}\""
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
